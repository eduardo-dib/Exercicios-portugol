programa {
	funcao inicio() {
	    /*Esse algoritmo calcula a média de 3 números inteiros*/
	   inteiro num1, num2, num3
	   
	   escreva("Escreva um número: ")
	   leia(num1)
	   
	    escreva("Escreva o segundo número: ")
	   leia(num2)
	   
	    escreva("Escreva um número: ")
	   leia(num3)
	   
	   escreva("A média entre esses números é: ", (num1+num2+num3)/3)
	}
}
